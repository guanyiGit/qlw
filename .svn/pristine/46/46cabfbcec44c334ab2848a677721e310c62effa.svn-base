.header-r-2{margin: 0 10px 0 15px;}
.header-r-3 a{ background: url(../images/h6.png) no-repeat left 10px;text-indent: 34px;}
.main{height: 956px;background: url(../images/bg2.jpg) no-repeat;position: relative;margin-top: 124px;}
.main-z{width: 1240px;height: 610px;position: absolute;left: 50%;top: 50%;margin: -305px 0 0 -620px;}
.main-z ul{overflow: hidden;}
.main-z ul li{width: 600px;height: 285px;float: left;}
.main-z ul li:nth-child(1){background: url(../images/bgh1.png) no-repeat;}
.main-z ul li:nth-child(2){background: url(../images/bgh2.png) no-repeat;}
.main-z ul li:nth-child(3){background: url(../images/bgh3.png) no-repeat;}
.main-z ul li:nth-child(4){background: url(../images/bgh4.png) no-repeat;}
.main-z ul li>a{height: 58px;line-height: 58px;color: #fffefe;font-size: 24px;}
.main-z ul li>a img{vertical-align: middle;margin-right: 10px}
.main-z ul li>a:nth-child(1){margin: 40px auto 20px;}
.main-z ul li:nth-child(4) a:nth-child(1){margin: 100px auto ;}
.main-z ul li p{font-size: 18px;color: #fffefe;}
.main-z ul li>div a{color: #fffefe;font-size: 18px;height: 22px;line-height: 22px;}
.main-z ul li>div a img{vertical-align: middle;margin-right: 10px;}
.main-z ul li:hover{-moz-box-shadow:0 0 5px #000;-webkit-box-shadow:0 0 5px #000; box-shadow:0 0 5px #000;}
.main-z ul li>a,
.main-z ul li>div a{
	transition: All .7s ease-in-out;
	-webkit-transition: All .7s ease-in-out;
	-moz-transition: All .7s ease-in-out;
	-o-transition: All .7s ease-in-out;
}
.main-z ul li>a:hover,
.main-z ul li>div a:hover{
	transform: translate(15px, 0);
	-webkit-transform: translate(15px, 0);
	-moz-transform: translate(15px, 0);
	-o-transform: translate(15px, 0);
	-ms-transform: translate(15px, 0);
	filter: alpha(opacity=60);-moz-opacity: 0.6;-khtml-opacity: 0.6;opacity: 0.6;
}
