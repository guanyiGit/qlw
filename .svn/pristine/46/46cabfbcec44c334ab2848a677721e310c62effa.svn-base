.zhe-z4{position: relative;left: 50%;top: 50%;margin: -185px 0 0 -280px; width: 560px;height: 370px;background-color: #fff;padding:30px 40px;}
.close{width: 28px;height: 28px;position: absolute;right: -14px;top: -14px;}
.s8{padding: 40px;}
.hr{height: 1px!important;background-color: #ddd;}
.div3{}
.div33{position: relative;border: 1px solid #ddd;width: 90%;height: 70px;}
.div33-1{position: absolute;right: -12.5px;top: -12.5px;;}
.div33-2{line-height: 70px;text-align: center;color: #999;float: left;width: 20%;border-right: 1px solid #ddd;}
.div33-2 span{display: inline-block;}
.div4{position: relative;width: 90%;height: 70px;line-height: 70px;overflow: hidden;}
.div4 div{float: left;width: 16.66%;color: #999;}
.div4 div:nth-child(-n+4){background: url(../images/btn3.png) 15px center no-repeat; text-indent: 53px;}
.div4 div:nth-child(n+5){background: url(../images/btn4.png) 15px center no-repeat; text-indent: 53px;}
/*================营收列表==================*/
.list4{}
.list4 li{height: 60px;line-height: 60px;}
.list4 li a{border-left: 5px solid #eee;text-indent: 110px;color: #666;font-size: 16px;}
.list4 li:nth-child(1) a{background: url(../images/k8.png) no-repeat 68px center;}
.list5 li:nth-child(1) a{background: url(../images/k10.png) no-repeat 68px center;}
.list4 li:nth-child(2) a{background: url(../images/k9.png) no-repeat 68px center;}
.list4 li:nth-child(1).active a,
.list4 li:nth-child(1):hover a{background:#fff url(../images/k8-b.png) no-repeat 68px center;}
.list5 li:nth-child(1).active a,
.list5 li:nth-child(1):hover a{background:#fff url(../images/k10-b.png) no-repeat 68px center;}
.list4 li:nth-child(2).active a,
.list4 li:nth-child(2):hover a{background:#fff url(../images/k9-b.png) no-repeat 68px center;}
.list4 li.active a,
.list4 li:hover a{border-left: 5px solid #03a9f4;}
.tx{font-size: 18px;color: #333;}
.tx span{font-size: 36px;color: #03a9f4;display: inline-block;margin: 0 10px;}
.tx a{width: 132px;height: 40px;background-color: #03a9f4;color: #fff;font-size: 16px;line-height: 40px;text-align: center;border-radius: 6px;display: inline-block;float: right;}
.s9{padding: 40px;}
/*===============终端店铺公众号==================*/
.s10{padding: 40px;}
.s10-1 a{display: inline-block;margin: 0 40px;}
.s10-1 a:hover{filter: alpha(opacity=60);-moz-opacity: 0.6;-khtml-opacity: 0.6;opacity: 0.6;}























































































































