package com.soholy.dogmanager.entity.dogCard;

import com.soholy.dogmanager.entity.TMemberCard;

import java.util.List;

//犬主所有详情信息 犬主档案里的详情
public class DogOwnerAllInfo {
    private static final long serialVersionUID = 1L;

    private TMemberCard tMemberCard;//会员证对象
    private DogOwners dogOwners;//描述犬主的信息对象
    private List<MyDog> myDogs;//我的犬只列表

    public TMemberCard gettMemberCard() {
        return tMemberCard;
    }

    public void settMemberCard(TMemberCard tMemberCard) {
        this.tMemberCard = tMemberCard;
    }

    public DogOwners getDogOwners() {
        return dogOwners;
    }

    public void setDogOwners(DogOwners dogOwners) {
        this.dogOwners = dogOwners;
    }

    public List<MyDog> getMyDogs() {
        return myDogs;
    }

    public void setMyDogs(List<MyDog> myDogs) {
        this.myDogs = myDogs;
    }

    @Override
    public String toString() {
        return "DogOwnerAllInfo{" +
                "tMemberCard=" + tMemberCard +
                ", dogOwners=" + dogOwners +
                ", myDogs=" + myDogs +
                '}';
    }
}
