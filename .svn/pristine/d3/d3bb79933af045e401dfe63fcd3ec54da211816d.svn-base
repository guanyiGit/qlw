	private Long operatorId;   //发布人
	
	private String pubishOrg;  //发布机构

	private String informationTypeName;

	private String infoTitile;

	private String childTitile;

	private Integer viewCount;

	private Integer hot;

	private Integer seq;

	private Integer infoState;

	private String failReason;

	private Date updateTime;

	private Date creationTime;
	private Date approvalTime;

	private String infoContent;

	private Integer mark;
	
	private List<TImages> images;
	
	public Date getApprovalTime() {
		return approvalTime;
	}

	public void setApprovalTime(Date approvalTime) {
		this.approvalTime = approvalTime;
	}

	
	
	public Long getInformationId() {
		return informationId;
	}

	public void setInformationId(Long informationId) {
		this.informationId = informationId;
	}

	public Long getOperatorId() {
		return operatorId;
	}

	public void setOperatorId(Long operatorId) {
		this.operatorId = operatorId;
	}
	
	public String getPubishOrg() {
		return pubishOrg;
	}

	public void setPubishOrg(String pubishOrg) {
		this.pubishOrg = pubishOrg;
	}

	public String getInformationTypeName() {
		return informationTypeName;
	}

	public void setInformationTypeName(String informationTypeName) {
		this.informationTypeName = informationTypeName;
	}

	public String getInfoTitile() {
		return infoTitile;
	}

	public void setInfoTitile(String infoTitile) {
		this.infoTitile = infoTitile;
	}

	public String getChildTitile() {
		return childTitile;
	}

