package com.soholy.dogmanager.entity.notice;

import com.soholy.dogmanager.entity.TNotification;
import com.soholy.dogmanager.entity.TUsers;

public class TNotificationDetail extends TNotification {

    /**
     * 发送人
     */
    private TUsers senderUser;

    /**
     * 接收人
     */
    private TUsers receiverUser;

    public TUsers getSenderUser() {
        return senderUser;
    }

    public void setSenderUser(TUsers senderUser) {
        this.senderUser = senderUser;
    }

    public TUsers getReceiverUser() {
        return receiverUser;
    }

    public void setReceiverUser(TUsers receiverUser) {
        this.receiverUser = receiverUser;
    }
}
